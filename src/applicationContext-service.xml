		<property name="credooScore" value="550"/>
		<property name="rskScore" value="60"/>
	</bean>
	
	<bean id="jxlEntityService" class="com.soft.ol.service.JxlEntityService">
		<property name="jxlEntityDao">
			<ref bean="jxlEntityDao"/>
		</property>
	</bean>
	
